{
  "name": "sukinime_backend",
  "version": "6.1.0",
  "description": "Sukinime Backend - Anime Scraper + Puppeteer Integration for Direct Video Links",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "anime",
    "scraper",
    "otakudesu",
    "sankavollerei",
    "puppeteer",
    "streaming",
    "video-extraction",
    "blogger",
    "googlevideo",
    "mp4",
    "hls"
  ],
  "author": "Sukinime Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "puppeteer": "^23.5.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=20.18.1",
    "npm": ">=10.x"
  },
  "puppeteer": {
    "args": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--disable-gpu",
      "--no-zygote"
    ],
    "executablePath": "/usr/bin/chromium-browser",
    "skipDownload": true
  },
  "overrides": {
    "undici": "5.28.4"
  }
}
